<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.suph.security.MemberDAO">

	<resultMap type="com.suph.security.MemberInfo" id="MemberInfo">
		<result column="MEM_NO" property="no"/>
		<result column="MEM_ID" property="id"/>
		<result column="MEM_PASSWORD" property="password"/>
		<result column="MEM_NICKNM" property="name"/>
		<result column="MEM_ENABLE" property="enable"/>		
	</resultMap>
	
	<insert id="insertMemverVO" parameterType="MemberInfo">
		INSERT INTO
			TB_MEMBER(
				MEM_ID,
				MEM_PASSWORD,
				MEM_NICKNM,
				MEM_ENABLE
			)
		VALUES(
			#{id},
			#{password},
			#{name},
			'Y'
		)
	</insert>
	
</mapper>